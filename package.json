    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/ui": "^4.0.16",
    "eslint": "^8",
    "eslint-config-next": "^14.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5",
    "vitest": "^4.0.16"
